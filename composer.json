{
  "name": "b2pweb/bdf-prime",
  "description": "Bdf Prime ORM",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "SÃ©bastien Tanneux"
    }
  ],
  "autoload": {
    "psr-4": {
      "Bdf\\Prime\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Php74\\": "tests/Php74"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.1",
    "ext-mbstring": "*",
    "b2pweb/bdf-dsn": "~1.0",
    "b2pweb/bdf-event-notifier": "~1.0",
    "b2pweb/bdf-serializer": "~1.0",
    "b2pweb/bdf-util": "~1.0",
    "doctrine/dbal": "~2.5",
    "doctrine/inflector": "~1.0",
    "psr/container": "~1.0",
    "psr/log": "~1.0",
    "psr/simple-cache": "^1.0",
    "symfony/filesystem": "~4.3|~5.0"
  },
  "conflict": {
    "doctrine/cache": ">=2.0"
  },
  "require-dev": {
    "b2pweb/bdf-phpunit": "~1.0",
    "cache/array-adapter": "~1.0",
    "phpunit/phpunit": "~7.0|~8.0",
    "symfony/console": "~4.3|~5.0",
    "symfony/http-foundation": "~4.3|~5.0",
    "symfony/http-kernel": "~4.3|~5.0",
    "symfony/validator": "~4.3|~5.0",
    "vimeo/psalm": "~4.6.0"
  },
  "scripts": {
    "tests": "phpunit",
    "tests-with-coverage": "phpunit --coverage-clover coverage.xml",
    "psalm": "psalm --shepherd"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.2-dev"
    }
  },
  "suggest": {
    "b2p/bdf-console": "required to use prime commands (~1.5)",
    "b2p/bdf-web": "required to use service provider (~1.5)",
    "phpunit/phpunit": "required to use repository assertion"
  }
}
